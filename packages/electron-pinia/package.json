{
  "name": "@packages/electron-pinia",
  "version": "0.0.0",
  "dependencies": {
    "@packages/toolbox": "^0.0.0",
    "electron": "22.3.25",
    "electron-store": "^8.1.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.21",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "microbundle": "^0.15.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1"
  },
  "exports": {
    ".": {
      "node": "./dist/renderer.cjs",
      "import": "./dist/renderer.js",
      "require": "./dist/renderer.cjs",
      "default": "./dist/renderer.cjs"
    },
    "./main": {
      "node": "./dist/main.cjs",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "default": "./dist/main.cjs"
    },
    "./renderer": {
      "node": "./dist/renderer.cjs",
      "import": "./dist/renderer.js",
      "require": "./dist/renderer.cjs",
      "default": "./dist/renderer.cjs"
    },
    "./preload": {
      "node": "./dist/preload.cjs",
      "import": "./dist/preload.js",
      "require": "./dist/preload.cjs",
      "default": "./dist/preload.cjs"
    },
    "./dist/": "./dist/"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/renderer.cjs",
  "module": "./dist/renderer.js",
  "private": true,
  "scripts": {
    "build": "microbundle src/{main,renderer,preload}.ts --target=node --format=esm,cjs  --sourcemap=false",
    "dev": "microbundle src/{main,renderer,preload}.ts --target=node --format=esm,cjs  --sourcemap=false watch"
  },
  "source": "src/renderer.ts",
  "type": "module",
  "types": "./dist/renderer.d.ts"
}
